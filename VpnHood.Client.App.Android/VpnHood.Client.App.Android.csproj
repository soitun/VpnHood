<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0-android34.0</TargetFramework>
		<OutputType>Exe</OutputType>
		<Nullable>enable</Nullable>
		<ApplicationId>com.vpnhood.client.android.web</ApplicationId>
		<ApplicationVersion>1</ApplicationVersion>
		<ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
		<RootNamespace>VpnHood.Client.App.Droid</RootNamespace>
		<SupportedOSPlatformVersion>23.0</SupportedOSPlatformVersion>
		<IncludeSymbols>True</IncludeSymbols>
	</PropertyGroup>

	<PropertyGroup>
		<Product>VpnHood.Client.App.Android</Product>
		<Description>VpnHood Android Client App</Description>
		<PackageLicenseExpression>LGPL-2.1-only</PackageLicenseExpression>
		<Company>OmegaHood LLC</Company>
		<Copyright>© OmegaHood LLC. All rights reserved.</Copyright>
		<PackageProjectUrl>https://github.com/vpnhood/vpnhood</PackageProjectUrl>
		<PackageIcon>VpnHood.png</PackageIcon>
		<RepositoryUrl>https://github.com/vpnhood/vpnhood</RepositoryUrl>
		<Version>3.0.418</Version>
		<FileVersion>$([System.DateTime]::Now.ToString("yyyy.M.d.HHmm"))</FileVersion>
	</PropertyGroup>

	<ItemGroup>
	  <AndroidResource Remove="Resources\values\colors.xml" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Remove="Ads\InterstitialAdLoadCallback.cs" />
	  <Compile Remove="Ads\VpnHoodAdActivity.cs" />
	  <Compile Remove="Ads\VpnHoodInterstitialAdLoadCallback.cs" />
	</ItemGroup>

	<ItemGroup>
	  <None Remove="AndroidManifest.aab.xml" />
	  <None Remove="Resources\values\colors.xml" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="Ads\InterstitialAdLoadCallback.cs" />
	  <None Include="Ads\VpnHoodAdActivity.cs" />
	  <None Include="Ads\VpnHoodInterstitialAdLoadCallback.cs" />
	</ItemGroup>
	
	<ItemGroup>
		<ProjectReference Include="..\VpnHood.Client.App.Resources\VpnHood.Client.App.Resources.csproj" />
		<ProjectReference Include="..\VpnHood.Client.App.WebServer\VpnHood.Client.App.WebServer.csproj" />
		<ProjectReference Include="..\VpnHood.Client.App\VpnHood.Client.App.csproj" />
		<ProjectReference Include="..\VpnHood.Client.Device.Android\VpnHood.Client.Device.Android.csproj" />
	</ItemGroup>
	
</Project>