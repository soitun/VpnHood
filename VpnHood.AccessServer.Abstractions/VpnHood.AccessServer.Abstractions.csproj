<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>VpnHood Access Server Abstractions</Description>
		<TargetFramework>netstandard2.1</TargetFramework>
		<IsPackable>true</IsPackable>
	</PropertyGroup>

	<PropertyGroup>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IncludeSymbols>True</IncludeSymbols>
		<SymbolPackageFormat>symbols.nupkg</SymbolPackageFormat>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<PackageLicenseExpression>LGPL-2.1-only</PackageLicenseExpression>
		<PackageTags>Vpn VpnHood AccessServer</PackageTags>
		<Company>OmegaHood LLC</Company>
		<Copyright>Â© OmegaHood LLC. All rights reserved.</Copyright>
		<PackageIcon>VpnHood.AccessServer.png</PackageIcon>
		<Version>3.0.223.0-alpha</Version>
		<FileVersion>$([System.DateTime]::Now.ToString("yyyy.M.d.HHmm"))</FileVersion>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\VpnHood.AccessServer.png" Link="Resources\VpnHood.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="VpnHood.Common" Version="4.6.553" />
	</ItemGroup>

</Project>