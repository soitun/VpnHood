    </Reference>
    <Reference Include="GrayMint.Authorization.RoleManagement.Abstractions">
      <HintPath>..\..\..\GrayMint\GrayMint.Authorization\GrayMint.Authorization.RoleManagement.Abstractions\bin\Debug\netstandard2.1\GrayMint.Authorization.RoleManagement.Abstractions.dll</HintPath>
    </Reference>
    <Reference Include="GrayMint.Authorization.RoleManagement.RoleAuthorizations">
      <HintPath>..\..\..\GrayMint\GrayMint.Authorization\GrayMint.Authorization.RoleManagement.RoleAuthorizations\bin\Debug\net7.0\GrayMint.Authorization.RoleManagement.RoleAuthorizations.dll</HintPath>
    </Reference>
    <Reference Include="GrayMint.Authorization.RoleManagement.SimpleRoleProviders">
      <HintPath>..\..\..\GrayMint\GrayMint.Authorization\GrayMint.Authorization.RoleManagement.SimpleRoleProviders\bin\Debug\net7.0\GrayMint.Authorization.RoleManagement.SimpleRoleProviders.dll</HintPath>
    </Reference>
    <Reference Include="GrayMint.Authorization.RoleManagement.TeamControllers">
      <HintPath>..\..\..\GrayMint\GrayMint.Authorization\GrayMint.Authorization.RoleManagement.TeamControllers\bin\Debug\net7.0\GrayMint.Authorization.RoleManagement.TeamControllers.dll</HintPath>
