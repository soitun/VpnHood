		<PackageReference Include="PacketDotNet" Version="1.4.7" />
		<PackageReference Include="System.Text.Json" Version="7.0.3" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\VpnHood.Common\VpnHood.Common.csproj" />
